%h1 New upload

= render 'form'

%div{:id => 'uploadOptions'}
	%button#browse{href: '#', onclick: 'popUp(); return false;', :type => "button"} File Format Info

%div{:id => 'popupWindow'}
	%div{:id => 'popupContent'}
		%h1 Required Header Names
		%p Please take care to match exact spelling and capitalization. Order does not matter.
		%table
			%tr
				%td Business Name
				%td Employer Identification Number (Federal EIN)
				%td NAICS
				%td Business Identification Number (BIN issued by Oregon Employment Department)
				%td Zip Code
				%td City
				%td County
				%td Business Type
				%td Jobs Retained
				%td Business Owner Name 1
				%td Business Owner Name 2
				%td Business Owner Name 3
				%td % Ownership 1
				%td % Ownership 2
				%td % Ownership 3
				%td Race 1
				%td Race 2
				%td Race 3
				%td Ethnicity 1
				%td Ethnicity 2
				%td Ethnicity 3
				%td Gender 1
				%td Gender 2
				%td Gender 3
				%td Total $ Approved:
		%button#close{href: '#', onclick: 'closePopUp(); return false;', :type => "button"} Close

= link_to 'Back', uploads_path
