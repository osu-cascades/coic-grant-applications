%p#notice= notice
%table
  %tr
    %th Round
    %th Company Name
    %th Jobs Retained
    %th Amount Approved
    %th EIN
    %th Date Uploaded
  - @applications.each do |application|
    %tr
      %td= application.round
      %td= get_company(application.ein).business_name
      %td= application.jobs_retained
      %td= application.amount_approved
      %td= application.ein
      %td= application.created_at


= link_to 'Edit', edit_upload_path(@upload)
\|
= link_to 'Back', uploads_path
